{
  "version": 2,
  "builds": [
    {
      "src": "index.js",
      "use": "@now/node"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "routes/auth.js"
    },
    {
      "src": "/(.*)",
      "dest": "routes/customers.js"
    },
    {
      "src": "/(.*)",
      "dest": "routes/genres.js"
    },
    {
      "src": "/(.*)",
      "dest": "routes/movies.js"
    },
    {
      "src": "/(.*)",
      "dest": "routes/rentals.js"
    },
    {
      "src": "/(.*)",
      "dest": "routes/users.js"
    }
  ]
}
